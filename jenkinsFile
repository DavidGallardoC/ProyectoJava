import java.time.LocalDate
def fecha = new LocalDate.now()
pipeline 
{
    agent any

    stages {
        stage('lunes') 
        {
            steps
            {
                script
                {
                def num1 = 5
                def num2 = 4
                
                println("La suma de " + num1 + " y " + num2 + " es " + (num1+num2))
                
                
                }
            }
        }
        stage('martes') 
        {
            steps
            {
                script
                {
                def num1 = 5
                def num2 = 4
                
                println("La resta de " + num1 + " y " + num2 + " es " + (num1-num2))
                
                
                }
            }
        }
        stage('miercoles') 
        {
            steps
            {
                script
                {
                def num1 = 5
                def num2 = 4
                
                println("La multiplicación de " + num1 + " y " + num2 + " es " + (num1*num2))
                
                
                }
            }
        }
          stage('jueves') 
        {
            steps
            {
                script
                {
                def num1 = 5
                def num2 = 4
                
                println("La multiplicación de " + num1 + " y " + num2 + " es " + (num1*num2))
                
                
                }
            }
        }
        stage('viernes') 
        {
            steps
            {
                script
                {
                def num1 = 5
                def num2 = 4
                
                println("La división de " + num1 + " y " + num2 + " es " + (num1/num2))
                
                
                }
            }
        }
    }
}
