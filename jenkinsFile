pipeline 
{
    agent any

    stages {
        stage('suma') 
        {
            steps
            {
                script
                {
                def num1 = 5
                def num2 = 4
                
                println("La suma de " + num1 + " y " + num2 + " es " + (num1+num2))
                
                
                }
            }
        }
